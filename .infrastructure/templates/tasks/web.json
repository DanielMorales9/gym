[
  {
    "essential": true,
    "image": "${image_url}",
    "memory": 384,
    "name": "${container_name}",
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 8080
      }
    ],
    "environment" : [],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group_name}",
        "awslogs-region": "${log_group_region}",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }
]
